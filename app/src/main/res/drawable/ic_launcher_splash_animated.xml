<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/ic_launcher_animated">

    <!-- Arrow drop animation -->
    <target android:name="arrowGroup">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="translateY"
                android:duration="800"
                android:valueFrom="-30"
                android:valueTo="0"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/decelerate_quad"/>
        </aapt:attr>
    </target>

    <!-- Z letter fade in -->
    <target android:name="zLetter">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="600"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:startOffset="200"
                android:interpolator="@android:interpolator/accelerate_decelerate"/>
        </aapt:attr>
    </target>

    <!-- Left bracket fade in -->
    <target android:name="leftBracket">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="strokeAlpha"
                android:duration="500"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:startOffset="400"
                android:interpolator="@android:interpolator/accelerate_decelerate"/>
        </aapt:attr>
    </target>

    <!-- Right bracket fade in -->
    <target android:name="rightBracket">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="strokeAlpha"
                android:duration="500"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:startOffset="400"
                android:interpolator="@android:interpolator/accelerate_decelerate"/>
        </aapt:attr>
    </target>

    <!-- Binary dots blinking sequentially -->
    <target android:name="dot1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="400"
                android:valueFrom="0.2"
                android:valueTo="1"
                android:valueType="floatType"
                android:startOffset="900"
                android:repeatCount="infinite"
                android:repeatMode="reverse"
                android:interpolator="@android:interpolator/accelerate_decelerate"/>
        </aapt:attr>
    </target>

    <target android:name="dot2">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="400"
                android:valueFrom="0.2"
                android:valueTo="1"
                android:valueType="floatType"
                android:startOffset="1000"
                android:repeatCount="infinite"
                android:repeatMode="reverse"
                android:interpolator="@android:interpolator/accelerate_decelerate"/>
        </aapt:attr>
    </target>

    <target android:name="dot3">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="400"
                android:valueFrom="0.2"
                android:valueTo="1"
                android:valueType="floatType"
                android:startOffset="1100"
                android:repeatCount="infinite"
                android:repeatMode="reverse"
                android:interpolator="@android:interpolator/accelerate_decelerate"/>
        </aapt:attr>
    </target>

    <target android:name="dot4">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="400"
                android:valueFrom="0.2"
                android:valueTo="1"
                android:valueType="floatType"
                android:startOffset="1200"
                android:repeatCount="infinite"
                android:repeatMode="reverse"
                android:interpolator="@android:interpolator/accelerate_decelerate"/>
        </aapt:attr>
    </target>

    <target android:name="dot5">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="400"
                android:valueFrom="0.2"
                android:valueTo="1"
                android:valueType="floatType"
                android:startOffset="1300"
                android:repeatCount="infinite"
                android:repeatMode="reverse"
                android:interpolator="@android:interpolator/accelerate_decelerate"/>
        </aapt:attr>
    </target>

</animated-vector>
